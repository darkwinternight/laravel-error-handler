{
	"name": "darkwinternight/laravel-error-handler",
	"description": "Error handler package for the Laravel 5 framework",
	"homepage": "https://github.com/darkwinternight/laravel-error-handler",
	"keywords": [ "errorhandler", "error", "exception", "debug", "laravel", "laravel 5", "whoops", "ajax" ],
	"license": "MIT",
	"authors": [
		{
			"name": "Alexander Graf",
			"email": "a.graf@aetherworld.org"
		}
	],
	"require": {
		"php": ">=5.6.0",
		"filp/whoops": ">=2.1.2",
		"laravel/framework": "5.2.*|5.3.*",
		"illuminate/support": "5.2.*|5.3.*"
	},
	"autoload": {
		"psr-4": {
			"Winternight\\LaravelErrorHandler\\": "src/Winternight/LaravelErrorHandler"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	},
	"minimum-stability": "dev"
}
